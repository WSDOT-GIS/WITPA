{
  "name": "witpa",
  "version": "1.0.0",
  "description": "WA Interactive Transportation Planning Application ",
  "main": "index.html",
  "scripts": {
    "test": "jasmine",
    "prepare": "npm run make-template-ts && npm run build:ts",
    "build:ts": "tsc -p src/browser && tsc -p src/common && tsc -p src/worker",
    "make-template-ts": "tsc -p build-scripts && node build-scripts/convertTemplateToTS.js",
    "old_prepublish": "bower install --force",
    "doc": "jsdoc2md ProjectFilter.js ProjectQueryManager.js conversionUtils.js infoWindowUtils.js wsdotMapUtils.js externals.js > api.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WSDOT-GIS/WITPA.git"
  },
  "keywords": [
    "CPDM",
    "WSDOT",
    "Washington",
    "WA",
    "planning",
    "transportation"
  ],
  "author": "WSDOT-GIS",
  "license": "Unlicense",
  "private": true,
  "bugs": {
    "url": "https://github.com/WSDOT-GIS/WITPA/issues"
  },
  "homepage": "https://github.com/WSDOT-GIS/WITPA#readme",
  "devDependencies": {
    "@esri/arcgis-rest-common-types": "^1.4.0",
    "@types/arcgis-js-api": "^3.24.0",
    "@types/dojo": "^1.9.40",
    "@types/html-minifier": "^3.5.2",
    "@types/jquery": "^3.3.2",
    "@types/node": "^10.3.1",
    "dialog-polyfill": "^0.4.9",
    "html-minifier": "^3.5.16",
    "jasmine": "^3.1.0",
    "jsdoc-to-markdown": "^4.0.1",
    "prettier": "^1.13.4",
    "ts-loader": "^4.3.1",
    "tslib": "^1.9.2",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.1",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.0.2"
  }
}
