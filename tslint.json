{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:recommended",
        "tslint-plugin-prettier",
        "tslint-config-prettier"
    ],
    "linterOptions": {
        "exclude": [
            "src/**/templates.ts"
        ]
    },
    "jsRules": {},
    "rules": {
        "prettier": true,
        "object-literal-sort-keys": false,
        "interface-name": {
            "options": [
                "never-prefix"
            ]
        },
        "variable-name": {
            "options": [
                "allow-leading-underscore"
            ]
        }
    },
    "rulesDirectory": []
}